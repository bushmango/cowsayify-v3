  "scripts": {
    "dev": "yarn start",
    "start": "stmux [ [ \"yarn dev-start\" ] : [ \"yarn dev-secondary\" .. \"yarn dev-test\" ] ] -t \"Cowsayify - :3005 - Ctrl+a then k to quit\" -e \"[Ff]ail(?!ed\\stests\\.)\" --mouse",
    "dev-secondary": "concurrently \"yarn sass\"",
    "dev-start": "yarn ts-node --project tsconfig.server.json server/index.ts",
    "build": "next build",
    "_start": "next start -p 8000",
    "start-prod": "cross-env NODE_ENV=production node dist/index.js",
    "now-build": "next build",
    "now-deploy": "npm run build && now",
    "sass-build": "tsm --nameFormat camel --exportType default '+(components|theme)/**/*.scss'",
    "sass": "tsm --watch --nameFormat camel --exportType default '+(components|theme)/**/*.scss'",
    "deploy-static": "s3-deploy './static/**' --cwd './' --region us-east-1 --bucket serverless-cowsay-3-prod",
    "deploy-dev": "awsume steve ;  SLS_DEBUG=* yarn cross-env STAGE=dev-001 sls deploy --stage dev-001",
    "deploy-prod": "awsume steve ;  yarn cross-env STAGE=prod-001 sls deploy --stage prod-001",
    "test": "jest -w 1",
    "dev-test": "jest --watch",
    "prettier-check": "prettier --check \"**/*.ts\"",
    "prettier-format": "prettier \"**/*.ts\" --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bushmango/cowsayify-v3.git"
