  "scripts": {
    "dev": "yarn start",
    "start": "stmux [ [ \"yarn dev-start\" ] : [ \"yarn dev-secondary\" .. \"yarn dev-test\" ] ] -t \"Cowsayify - :3004 - Ctrl+a then k to quit\" -e \"[Ff]ail(?!ed\\stests\\.)\" --mouse",
    "dev-secondary": "concurrently \"yarn sass\"",
    "dev-start": "tsc --project tsconfig.server.json  && nodemon server/index.js",
    "build": "next build",
    "_start": "next start -p 8000",
    "start-prod": "cross-env NODE_ENV=production node dist/index.js",
    "now-build": "next build",
    "now-deploy": "npm run build && now",
    "sass-build": "tsm --nameFormat camel --exportType default '+(pages|components)/**/*.scss'",
    "sass": "tsm --watch --nameFormat camel --exportType default '+(pages|components)/**/*.scss'",
    "deploy-static": "s3-deploy './static/**' --cwd './' --region us-east-1 --bucket serverless-cowsay-3-prod",
    "deploy-dev": "awsume steve && sls deploy --stage dev",
    "deploy-prod": "awsume steve && sls deploy --stage prod",
    "test": "jest -w 1",
    "dev-test": "jest --watch"
  },
  "repository": {
    "type": "git",
