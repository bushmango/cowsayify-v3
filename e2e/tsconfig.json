{
	"compilerOptions": {
		"incremental": true,
		"lib": ["dom", "es7"],
		"target": "es5",
		"module": "commonjs",
		"noImplicitAny": false,
		"noImplicitReturns": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"baseUrl": ".",
		"paths": {
			"*": ["./*"],
			"pages/*": ["./pages/*"],
			"models/*": ["./models/*"],
			"test/*": ["./test/*"]
		},
		"types": ["webdriverio", "mocha", "node", "chai"]
	},
	"include": ["./test/**/*.ts"],
	"exclude": ["./node_modules"],
	"compileOnSave": false
}
